   "source": [
    "class Solution:\n",
    "    def climbStairs(self, n: int) -> int:\n",
    "        dp = [0] * (n+2)\n",
    "        \n",
    "        dp[0] = 0\n",
    "        dp[1] = 1\n",
    "        dp[2] = 2\n",
    "        for i in range(3,n+2):\n",
    "            dp[i] += dp[i-1]\n",
    "            dp[i] += dp[i-2]\n",
    "        \n",
    "        return dp[n]\n",
    "        "
   ]
