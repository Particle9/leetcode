   "source": [
    "from typing import Optional, List\n",
    "\n",
    "class Solution:\n",
    "    def rob(self, nums: List[int]) -> int:\n",
    "        n = len(nums)\n",
    "        dp = [0] * (n+2)\n",
    "        dp[1] = nums[0]\n",
    "        for i in range(2,n+1):\n",
    "            dp[i] = max(dp[i-2] + nums[i-1], dp[i-1])\n",
    "        return dp[n]"
   ]
  }
