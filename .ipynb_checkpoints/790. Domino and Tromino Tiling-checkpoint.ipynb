   "source": [
    "class Solution:\n",
    "    def numTilings(self, n: int) -> int:\n",
    "        dp = [0] * max((n+1), 3)\n",
    "        dp[0] = 1\n",
    "        dp[1] = 1\n",
    "        dp[2] = 2\n",
    "        for i in range(3,n+1):\n",
    "            dp[i] = dp[i-1] * 2 + dp[i-3]\n",
    "        return dp[n] % (10**9 + 7)\n",
    "        "
   ]
  }
