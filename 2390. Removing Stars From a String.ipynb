   "source": [
    "class Solution:\n",
    "    def removeStars(self, s: str) -> str:\n",
    "        stack = []\n",
    "        for x in s:\n",
    "            if x != '*':\n",
    "                stack += [x]\n",
    "            else:\n",
    "                stack.pop()\n",
    "        return ''.join(stack)\n",
    "        "
   ]
  }
