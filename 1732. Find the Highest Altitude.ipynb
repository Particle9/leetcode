   "source": [
    "class Solution:\n",
    "    def largestAltitude(self, gain: List[int]) -> int:\n",
    "        currAltitude = 0\n",
    "        maxAltitude = 0\n",
    "        for g in gain:\n",
    "            currAltitude += g\n",
    "            maxAltitude = max(maxAltitude, currAltitude)\n",
    "        return maxAltitude\n",
    "        "
   ]
  }
