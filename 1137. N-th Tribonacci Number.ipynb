   "source": [
    "from typing import List\n",
    "\n",
    "class Solution:\n",
    "    def tribonacci(self, n: int) -> int:\n",
    "        dp = [0] * max((n+1), 3)\n",
    "        dp[0], dp[1], dp[2] = 0,1,1\n",
    "        for i in range(3,n+1):\n",
    "            dp[i] = dp[i-1] + dp[i-2] + dp[i-3]\n",
    "        \n",
    "        return dp[n]"
   ]
